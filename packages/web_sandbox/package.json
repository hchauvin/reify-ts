{
  "name": "web-sandbox",
  "version": "1.1.0",
  "description": "Web sandbox to try out reify-ts",
  "repository": "git@github.com:hchauvin/reify-ts.git",
  "author": "Hadrien Chauvin",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "antd": "^3.16.3",
    "codemirror": "^5.45.0",
    "react": "^16.8.6",
    "react-codemirror2": "^7.0.0",
    "react-dom": "^16.8.3",
    "reify-ts": "^1.1.0",
    "ttypescript": "^1.5.7"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.6.0",
    "@types/codemirror": "^0.0.91",
    "@types/node": "^12.7.12",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.3",
    "@types/terser-webpack-plugin": "^2.0.0",
    "@types/webpack-dev-server": "^3.1.1",
    "css-loader": "^3.0.0",
    "fast-check": "^1.5.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.13.7",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terminate": "^2.1.2",
    "terser-webpack-plugin": "^3.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-language-service": "^0.9.8",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "3.6.4",
    "url-loader": "^4.0.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  },
  "scripts": {
    "clean": "rimraf build",
    "ts": "ttsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write --ignore-path .gitignore '**/*.ts'",
    "format:check": "prettier -l --list-different --ignore-path .gitignore '**/*.ts'",
    "test": "ava",
    "all": "yarn clean && yarn format:check && yarn lint && yarn ts && yarn web:build",
    "precommit": "lint-staged",
    "web": "webpack-dev-server --config webpack.config",
    "web:build": "webpack-cli -p --config webpack.config",
    "web:deploy": "yarn gh-pages -d src/client/public -m '[ci skip] Updates'",
    "doc": "echo 'SKIPPED'"
  },
  "ava": {
    "files": [
      "build/**/__tests__/*.{test,it}.js"
    ]
  },
  "lint-staged": {
    "{./*,typescript/**/*}.{js,jsx,ts,tsx,css,scss,md,json}": "prettier -l",
    "*.{ts,tsx}": "tslint -c tslint.json"
  },
  "sideEffects": false,
  "keywords": [
    "typescript",
    "reflection",
    "reification",
    "type erasure",
    "API",
    "I/O boundary"
  ]
}
