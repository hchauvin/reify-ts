{
  "name": "web-sandbox",
  "version": "1.0.2",
  "description": "Web sandbox to try out reify-ts",
  "repository": "git@github.com:hchauvin/reify-ts.git",
  "author": "Hadrien Chauvin",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "antd": "^3.16.3",
    "codemirror": "^5.45.0",
    "fast-text-encoding": "^1.0.0",
    "fetch-suspense": "^1.0.2",
    "grpc-web-client": "^0.6.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.14.1",
    "node-jose": "^1.0.0",
    "puppeteer": "^1.6.0-next.1532381444032",
    "query-string": "^6.0.0",
    "react": "^16.8.6",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.8.3",
    "reify-ts": "^1.0.2",
    "tsconfig-paths": "3.7.0",
    "ttypescript": "^1.5.6",
    "typescript": "3.4.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/codemirror": "^0.0.74",
    "@types/fetch-mock": "^6.0.3",
    "@types/lodash": "^4.14.106",
    "@types/node": "^8.0.47",
    "@types/puppeteer": "^1.5.1",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.3",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack-dev-server": "^3.1.1",
    "antd-scss-theme-plugin": "^1.0.7",
    "ava": "^1.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "fast-check": "^1.5.0",
    "fetch-mock": "^6.5.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.13.7",
    "raw-loader": "^2.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.20.3",
    "terminate": "^2.1.2",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^4.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-language-service": "^0.9.8",
    "tslint-no-circular-imports": "^0.6.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  },
  "scripts": {
    "clean": "rimraf build",
    "ts": "ttsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write --ignore-path .gitignore '**/*.ts'",
    "format:check": "prettier -l --list-different --ignore-path .gitignore '**/*.ts'",
    "test": "ava build/**/__tests__/*.{test,it}.js",
    "all": "yarn clean && yarn format:check && yarn lint && yarn ts && yarn web:build",
    "precommit": "lint-staged",
    "web": "webpack-dev-server --config webpack.config",
    "web:build": "webpack-cli -p --config webpack.config",
    "web:deploy": "yarn gh-pages -d src/client/public -m '[ci skip] Updates'",
    "doc": "echo 'SKIPPED'"
  },
  "lint-staged": {
    "{./*,typescript/**/*}.{js,jsx,ts,tsx,css,scss,md,json}": "prettier -l",
    "*.{ts,tsx}": "tslint -c tslint.json"
  },
  "sideEffects": false,
  "keywords": [
    "typescript",
    "reflection",
    "reification",
    "type erasure",
    "API",
    "I/O boundary"
  ]
}
