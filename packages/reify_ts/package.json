{
  "name": "reify-ts",
  "version": "1.0.0",
  "description": "TypeScript runtime type reflection",
  "repository": "git@github.com:hchauvin/reify-ts.git",
  "author": "Hadrien Chauvin",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "unpkg": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/immutable": "^3.8.7",
    "@types/uuid": "^3.4.4",
    "compression": "^1.0.3",
    "express": "^4.16.3",
    "fast-text-encoding": "^1.0.0",
    "grpc-web-client": "^0.6.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.14.1",
    "newtype-ts": "^0.2.4",
    "node-jose": "^1.0.0",
    "query-string": "^6.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.0.0",
    "@types/express": "^4.11.1",
    "@types/fetch-mock": "^6.0.3",
    "@types/fs-extra": "^5.0.5",
    "@types/lodash": "^4.14.106",
    "@types/node": "^8.0.47",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.8.10",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^2.3.7",
    "antd": "^3.15.2",
    "ava": "^1.4.1",
    "benchmark": "^2.1.4",
    "chai": "3.5.0",
    "codedown": "^2.1.5",
    "coveralls": "^3.0.2",
    "fast-check": "^1.5.0",
    "fetch-mock": "^6.5.0",
    "fp-ts": "^1.15.0",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "immutable": "^4.0.0-rc.12",
    "io-ts": "^1.8.5",
    "mobx": "^5.9.4",
    "nyc": "^14.0.0",
    "prettier": "^1.13.7",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.4.0",
    "sinon": "^4.0.1",
    "terminate": "^2.1.2",
    "ts-node": "^4",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-language-service": "^0.9.8",
    "tslint-no-circular-imports": "^0.6.1",
    "ttypescript": "^1.5.6",
    "type-zoo": "^3.3.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typelevel-ts": "^0.3.5",
    "typescript": "3.4.1",
    "vue": "^2.6.10"
  },
  "scripts": {
    "clean": "rimraf lib dist es typedoc",
    "test": "ava 'lib/**/__tests__/*.{test,it}.js' --verbose --concurrency=20",
    "cov": "yarn nyc --reporter=html --reporter=text ava 'lib/**/__tests__/*.{test,it}.js' --verbose --concurrency=20",
    "cov:upload": "yarn nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "perf": "node lib/benchmark",
    "build:es": "tsc -p tsconfig.json --declaration --module esnext --outDir ./es",
    "build:cjs": "tsc -p tsconfig.json --declaration --module commonjs",
    "build": "yarn run build:cjs && yarn run build:es",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write --ignore-path .gitignore '**/*.ts'",
    "format:check": "prettier -l --list-different --ignore-path .gitignore '**/*.ts'",
    "all": "yarn clean && yarn build && yarn format:check && yarn lint && yarn test",
    "precommit": "lint-staged",
    "doc": "typedoc --options typedoc.js src"
  },
  "lint-staged": {
    "{./*,typescript/**/*}.{js,jsx,ts,tsx,css,scss,md,json}": "prettier -l",
    "*.{ts,tsx}": "tslint -c tslint.json"
  },
  "npmName": "rpc_ts",
  "sideEffects": false,
  "keywords": [
    "typescript",
    "reflection",
    "reification",
    "type erasure",
    "API",
    "I/O boundary"
  ]
}
