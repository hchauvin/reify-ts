{
  "name": "example-webpack",
  "version": "1.0.4",
  "description": "Example usage of reify-ts with webpack",
  "repository": "git@github.com:hchauvin/reify-ts.git",
  "author": "Hadrien Chauvin",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "fetch-suspense": "1.0.2",
    "puppeteer": "1.14.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "reify-ts": "^1.0.4",
    "ttypescript": "1.5.7"
  },
  "devDependencies": {
    "@babel/polyfill": "7.4.3",
    "@types/lodash": "4.14.118",
    "@types/mocha": "2.2.48",
    "@types/node": "8.10.37",
    "@types/path-is-inside": "1.0.0",
    "@types/puppeteer": "1.12.3",
    "@types/query-string": "5.1.0",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "@types/sinon": "2.3.7",
    "@types/terser-webpack-plugin": "1.2.1",
    "@types/uuid": "3.4.4",
    "@types/webpack-dev-server": "3.1.5",
    "ava": "1.4.1",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "node-sass": "4.11.0",
    "postcss-loader": "2.1.6",
    "prettier": "1.15.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.20.3",
    "terminate": "2.1.2",
    "terser-webpack-plugin": "1.2.3",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-standard": "7.1.0",
    "tslint-language-service": "0.9.9",
    "tslint-no-circular-imports": "0.6.1",
    "typescript": "3.5.1",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "scripts": {
    "ts": "ttsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write --ignore-path .gitignore '**/*.ts'",
    "format:check": "prettier -l --list-different --ignore-path .gitignore '**/*.ts'",
    "test": "ava build/**/__tests__/*.{test,it}.js",
    "all": "yarn format:check && yarn lint && yarn ts && yarn test",
    "precommit": "lint-staged",
    "web": "webpack-dev-server --config webpack.config",
    "doc": "echo 'SKIPPED'",
    "heroku-postbuild": "yarn web:build"
  },
  "lint-staged": {
    "{./*,typescript/**/*}.{js,jsx,ts,tsx,css,scss,md,json}": "prettier -l",
    "*.{ts,tsx}": "tslint -c tslint.json"
  },
  "sideEffects": false,
  "keywords": [
    "typescript",
    "reflection",
    "reification",
    "type erasure",
    "API",
    "I/O boundary"
  ]
}
