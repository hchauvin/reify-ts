{
  "name": "example-webpack",
  "version": "1.1.0",
  "description": "Example usage of reify-ts with webpack",
  "repository": "git@github.com:hchauvin/reify-ts.git",
  "author": "Hadrien Chauvin",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "fetch-suspense": "^1.0.2",
    "puppeteer": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.3",
    "reify-ts": "^1.1.0",
    "ttypescript": "^1.5.7"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.6.0",
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^7.0.0",
    "@types/node": "^12.7.12",
    "@types/path-is-inside": "^1.0.0",
    "@types/puppeteer": "^2.0.0",
    "@types/query-string": "^6.0.0",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.3",
    "@types/sinon": "^7.0.0",
    "@types/terser-webpack-plugin": "^2.0.0",
    "@types/uuid": "^3.4.4",
    "@types/webpack-dev-server": "^3.1.1",
    "ava": "^2.4.0",
    "cross-env": "^6.0.0",
    "css-loader": "^3.0.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.13.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terminate": "^2.1.2",
    "terser-webpack-plugin": "^2.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-language-service": "^0.9.8",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "3.6.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  },
  "scripts": {
    "ts": "ttsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write --ignore-path .gitignore '**/*.ts'",
    "format:check": "prettier -l --list-different --ignore-path .gitignore '**/*.ts'",
    "test": "ava",
    "all": "yarn format:check && yarn lint && yarn ts && yarn test",
    "precommit": "lint-staged",
    "web": "webpack-dev-server --config webpack.config",
    "doc": "echo 'SKIPPED'",
    "heroku-postbuild": "yarn web:build"
  },
  "ava": {
    "files": [
      "build/**/__tests__/*.{test,it}.js"
    ]
  },
  "lint-staged": {
    "{./*,typescript/**/*}.{js,jsx,ts,tsx,css,scss,md,json}": "prettier -l",
    "*.{ts,tsx}": "tslint -c tslint.json"
  },
  "sideEffects": false,
  "keywords": [
    "typescript",
    "reflection",
    "reification",
    "type erasure",
    "API",
    "I/O boundary"
  ]
}
